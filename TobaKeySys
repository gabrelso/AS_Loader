local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/ttwizz/Roblox/master/Orion.lua", true))()

OrionLib:MakeNotification({
	Name = "Script descontinuado!",
	Content = "O script foi descontinudo e não receberá mais atualizações, uma versão paga poderá ser criada no futuro, com grandes aperfeiçoamentos.",
	Image = "rbxassetid://",
	Time = 30
})

